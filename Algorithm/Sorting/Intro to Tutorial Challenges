import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner in = new Scanner(System.in);
        int goal = in.nextInt();
        in.nextInt();
        int index = 0;
        while(in.hasNextInt()){
            if(in.nextInt() == goal) {
                System.out.println(index);
                break;
            }
            index++;
        }
    }
}
